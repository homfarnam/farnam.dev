{
  "name": "farnam.dev",
  "private": true,
  "scripts": {
    "dev": " next ",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "test": "jest",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "schemagen": "apollo client:download-schema --endpoint='https://farnam.tech/graphql' schema.graphqls",
    "jsongraph": "npx apollo service:download --endpoint=https://farnam.tech/graphql graphql-schema.json",
    "tsg": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql",
    "codegen": "yarn schemagen && graphql-let",
    "types": "apollo client:codegen --localSchemaFile=./schema.graphqls --includes=./graphql/**/*.ts --target=typescript",
    "generate": "graphql-codegen --config codegen.yml",
    "newlint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@apollo/react-hooks": "^4.0.0",
    "@loadable/component": "^5.15.2",
    "@types/loadable__component": "^5.13.4",
    "@types/styled-components": "^5.1.19",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "date-fns": "^2.28.0",
    "dotenv": "^8.2.0",
    "formik": "^2.1.5",
    "global": "^4.4.0",
    "graphql": "^15.3.0",
    "graphql-let": "^0.15.4",
    "graphql-tag": "^2.11.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^26.4.2",
    "markdown-to-jsx": "^7.1.7",
    "moment": "^2.28.0",
    "next": "latest",
    "next-babel-minify": "^1.0.0",
    "next-fonts": "^1.4.0",
    "next-with-apollo": "^5.1.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "prettier": "^2.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-indiana-drag-scroll": "^2.1.0",
    "sass": "^1.47.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/jit": "^0.1.18",
    "@types/marked": "^4.0.7",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "autoprefixer": "^10.4.12",
    "core-js": "^3.14.0",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-purgecss": "^4.0.0",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.5.4"
  }
}
